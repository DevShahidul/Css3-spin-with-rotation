@keyframes spin{
    from {
        -webkit-transform:rotate(0deg) translateX(50px);
        -moz-transform:rotate(0deg) translateX(50px);
        -os-transform:rotate(0deg) translateX(50px);
        -ms-transform:rotate(0deg) translateX(50px);
        transform:rotate(0deg) translateX(50px);
    }
    to {
        -webkit-transform:rotate(360deg) translateX(0px);
        -moz-transform:rotate(360deg) translateX(0px);
        -os-transform:rotate(360deg) translateX(0px);
        -ms-transform:rotate(360deg) translateX(0px);
        transform:rotate(360deg) translateX(0px);
    }
}

.sunlite,.triangle, .sun, .diamond-large, .gray-diamond-small{-webkit-animation-name: spin; -moz-animation-name: spin; animation-name: spin; -webkit-animation-duration: 18000ms; -moz-animation-duration: 18000ms; animation-duration: 18000ms; -webkit-animation-iteration-count: infinite; -moz-animation-iteration-count: infinite; animation-iteration-count: infinite; -webkit-animation-timing-function: linear; -moz-animation-timing-function: linear; animation-timing-function: linear; -webkit-animation-direction: alternate; -moz-animation-direction: alternate; animation-direction: alternate; }
